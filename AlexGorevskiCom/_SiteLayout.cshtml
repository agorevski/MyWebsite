<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
        <meta name="author" content="Alexander Gorevski">
        <meta name="description" content="">
        <meta name="keywords" content="">

        <title>Alexander T Gorevski | Personal Website & Resume</title>
        <!-- <link rel="icon" href="~/Images/site/fav-icon.webp"> -->
        <!-- <link rel="apple-touch-icon" href="~/Images/site/apple-touch-icon.webp"> -->
        
        <!-- Preconnect to Google Fonts for faster loading -->
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        
        <!-- Preload critical fonts -->
        <link rel="preload" as="font" type="font/woff2" href="~/Content/fonts/fontawesome-webfont.woff2" crossorigin>
        <link rel="preload" as="font" type="font/woff2" href="https://fonts.gstatic.com/s/raleway/v34/1Ptxg8zYS_SKggPN4iEgvnHyvveLxVvao4CPNLA3.woff2" crossorigin>
        <link rel="preload" as="font" type="font/woff2" href="https://fonts.gstatic.com/s/muli/v37/7Aulp_0qi-FZqHnHz5hf8xv-IF7lIg.woff2" crossorigin>
        
        <!-- Critical CSS - loaded synchronously -->
        <link href="~/Content/stylesheets/vendors/bootstrap.min.css" rel="stylesheet">
        <link href="~/Content/stylesheets/vendors/materialize.css" rel="stylesheet">
        <link href="~/Content/stylesheets/style.css" id="switch_style" rel="stylesheet">
        
        <!-- Non-critical CSS - loaded asynchronously -->
        <link href="~/Content/stylesheets/vendors/font-awesome.min.css" rel="stylesheet" media="print" onload="this.media='all'">
        <link href="~/Content/stylesheets/vendors/animate.min.css" rel="stylesheet" media="print" onload="this.media='all'">
        <link href="~/Content/stylesheets/vendors/magnific-popup.css" rel="stylesheet" media="print" onload="this.media='all'">
        <link href="~/Content/stylesheets/vendors/swiper.min.css" rel="stylesheet" media="print" onload="this.media='all'">
        <link href="~/Content/toggle/toggle.css" rel="stylesheet" media="print" onload="this.media='all'">
        
        <!-- Google Fonts - loaded asynchronously -->
        <link href='https://fonts.googleapis.com/css?family=Raleway:500&display=swap' rel='stylesheet' media="print" onload="this.media='all'">
        <link href='https://fonts.googleapis.com/css?family=Muli&display=swap' rel='stylesheet' media="print" onload="this.media='all'">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons&display=swap" rel="stylesheet" media="print" onload="this.media='all'">
        
        <!-- Fallback for browsers that don't support onload -->
        <noscript>
            <link href="~/Content/stylesheets/vendors/font-awesome.min.css" rel="stylesheet">
            <link href="~/Content/stylesheets/vendors/animate.min.css" rel="stylesheet">
            <link href="~/Content/stylesheets/vendors/magnific-popup.css" rel="stylesheet">
            <link href="~/Content/stylesheets/vendors/swiper.min.css" rel="stylesheet">
            <link href="~/Content/toggle/toggle.css" rel="stylesheet">
            <link href='https://fonts.googleapis.com/css?family=Raleway:500&display=swap' rel='stylesheet'>
            <link href='https://fonts.googleapis.com/css?family=Muli&display=swap' rel='stylesheet'>
            <link href="https://fonts.googleapis.com/icon?family=Material+Icons&display=swap" rel="stylesheet">
        </noscript>
        <meta name="viewport" content="width=device-width" />
    </head>
<body>
@RenderBody()


    <!-- FOOTER -->
    <footer>
        <div class="container">
            <p class="text-center">
                © Alex Gorevski 2017
            </p>
        </div>
    </footer>

    <!-- SCRIPTS -->
    <!-- Critical JS - loaded synchronously -->
    <script src="~/Content/javascript/vendors/jquery-2.1.3.min.js"></script>
    <script src="~/Content/javascript/vendors/bootstrap.min.js"></script>
    <script src="~/Content/javascript/vendors/materialize.min.js"></script>
    
    <!-- Non-critical JS - deferred for better performance -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDaNPZfQMdAHwTbVoqBAYshvj4-pI2pRjU" defer></script>
    <script src="~/Content/javascript/vendors/markerwithlabel.min.js" defer></script>
    <script src="~/Content/javascript/vendors/retina.min.js" defer></script>
    <script src="~/Content/javascript/vendors/scrollreveal.min.js" defer></script>
    <script src="~/Content/javascript/vendors/swiper.jquery.min.js" defer></script>
    <script src="~/Content/javascript/vendors/jquery.magnific-popup.min.js" defer></script>
    <script src="~/Content/javascript/custom.js" defer></script>
    <script src="~/Content/toggle/js.js" defer></script>
</body>
</html>
